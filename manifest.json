{
  "manifest_version": 3,
  "name": "Datepicker Range Console",
  "version": "1.0.0",
  "description": "Logs startUTC and endExclusiveUTC from range-datepicker-cell.",
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "background": { "service_worker": "bg.js" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames": true,
      "match_about_blank": true
    }
  ]
}
